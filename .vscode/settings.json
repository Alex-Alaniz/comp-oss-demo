{
	"prisma.fileWatcher": true,
	"[prisma]": {
		"editor.defaultFormatter": "Prisma.prisma"
	},
	"[javascript]": {
		"editor.defaultFormatter": "biomejs.biome"
	},
	"[typescript]": {
		"editor.defaultFormatter": "biomejs.biome"
	},
	"editor.defaultFormatter": "biomejs.biome",
	"editor.tabSize": 4,
	"editor.detectIndentation": true,
	"[yaml]": {
		"editor.tabSize": 4
	},
	"[dockercompose]": {
		"editor.tabSize": 4
	},
	"editor.codeActionsOnSave": {
		"source.fixAll.biome": "explicit"
	}
}
