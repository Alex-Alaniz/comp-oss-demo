model PortalSession {
  id        String     @id
  expiresAt DateTime
  token     String     @unique
  createdAt DateTime
  updatedAt DateTime
  ipAddress String?
  userAgent String?
  userId    String
}

model PortalVerification {
  id         String    @id
  identifier String
  value      String
  expiresAt  DateTime
  createdAt  DateTime?
  updatedAt  DateTime?
}

model OrganizationTrainingVideos {
  id              String   @id @default(uuid())
  organizationId  String
  trainingVideoId String
  completedBy     String[] @default([])

  organization  Organization         @relation(fields: [organizationId], references: [id], onDelete: Cascade)
}
