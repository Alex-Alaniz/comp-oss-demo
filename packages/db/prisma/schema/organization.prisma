model Organization {
  id               String      @id @default(dbgenerated("generate_prefixed_cuid('org'::text)"))
  name             String
  logo             String?
  createdAt        DateTime
  metadata         String?
  stripeCustomerId String?
  onboarding       Onboarding?
  website          String?

  // FleetDM
  fleetDmLabelId        Int?
  isFleetSetupCompleted Boolean @default(false)

  apiKeys            ApiKey[]
  auditLog           AuditLog[]
  controls           Control[]
  frameworkInstances FrameworkInstance[]
  integrations       Integration[]
  invitations        Invitation[]
  members            Member[]
  policy             Policy[]
  risk               Risk[]
  vendors            Vendor[]
  tasks              Task[]
  comments           Comment[]
  attachments        Attachment[]
  trust              Trust[]
  context            Context[]
}
