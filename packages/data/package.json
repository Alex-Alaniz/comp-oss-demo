{
	"name": "@bubba/data",
	"version": "1.0.0",
	"private": true,
	"main": "./dist/index.js",
	"module": "./dist/index.mjs",
	"types": "./dist/index.d.ts",
	"files": ["dist/**"],
	"scripts": {
		"build": "tsup src/index.ts --format cjs,esm --dts",
		"dev": "tsup src/index.ts --format cjs,esm --watch --dts",
		"lint": "eslint \"src/**/*.ts*\"",
		"clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist"
	},
	"devDependencies": {
		"tsup": "^8.0.1",
		"typescript": "^5.7.3"
	}
}
