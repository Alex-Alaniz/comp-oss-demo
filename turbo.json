{
  "$schema": "https://turborepo.org/schema.json",
  "globalDependencies": ["**/.env"],
  "ui": "stream",
  "tasks": {
    "build": {
      "env": [
        "AUTH_GOOGLE_ID",
        "AUTH_GOOGLE_SECRET",
        "AUTH_GITHUB_ID",
        "AUTH_GITHUB_SECRET",
        "AUTH_SECRET",
        "DATABASE_URL",
        "OPENAI_API_KEY",
        "RESEND_API_KEY",
        "UPSTASH_REDIS_REST_URL",
        "UPSTASH_REDIS_REST_TOKEN",
        "NEXT_PUBLIC_OPENPANEL_CLIENT_ID",
        "RESEND_API_KEY",
        "RESEND_AUDIENCE_ID",
        "NEXT_PUBLIC_GOOGLE_TAG_ID",
        "DISCORD_WEBHOOK_URL",
        "TRIGGER_SECRET_KEY",
        "VERCEL_AUTH_TOKEN",
        "VERCEL_TEAM_ID",
        "VERCEL_PROJECT_ID",
        "NEXT_PUBLIC_VERCEL_URL",
        "ZAP_WEBHOOK_URL",
        "BETTER_AUTH_SECRET",
        "BETTER_AUTH_URL",
        "APP_AWS_BUCKET_NAME",
        "APP_AWS_REGION",
        "APP_AWS_ACCESS_KEY_ID",
        "APP_AWS_SECRET_ACCESS_KEY",
        "ANTHROPIC_API_KEY",
        "REVALIDATION_SECRET",
        "GROQ_API_KEY",
        "RESEND_AUDIENCE_ID",
        "FIRECRAWL_API_KEY",
        "ZAPIER_HUBSPOT_WEBHOOK_URL",
        "TRUST_PORTAL_PROJECT_ID",
        "FLEET_URL",
        "FLEET_TOKEN",
        "FLEET_DEVICE_PATH_MAC",
        "FLEET_AGENT_BUCKET_NAME",
        "FLEET_DEVICE_PATH_WINDOWS",
        "LOGO_DEV",
        "DUB_API_KEY",
        "DUB_PROGRAM_ID"
      ],
      "inputs": ["$TURBO_DEFAULT$", ".env"],
      "dependsOn": ["^build", "^db:generate", "^auth:build"],
      "outputs": [".next/**", "!.next/cache/**", "next-env.d.ts", "data:build"]
    },
    "db:generate": {
      "cache": false
    },
    "db:migrate": {
      "cache": false,
      "persistent": true
    },
    "auth:build": {
      "cache": false,
      "env": ["BETTER_AUTH_SECRET", "BETTER_AUTH_URL", "AUTH_GOOGLE_ID", "AUTH_GOOGLE_SECRET"]
    },
    "data:build": {
      "cache": false,
      "env": ["DATABASE_URL"],
      "inputs": ["$TURBO_DEFAULT$", ".env"],
      "dependsOn": ["^db:generate"],
      "outputs": ["data:build"]
    },
    "db:push": {
      "cache": false
    },
    "clean": {
      "cache": false
    },
    "integrations:dev": {
      "cache": false
    },
    "dev": {
      "inputs": ["$TURBO_DEFAULT$", ".env"],
      "dependsOn": ["^db:generate"],
      "persistent": true,
      "cache": false
    },
    "lint": {
      "dependsOn": []
    },
    "typecheck": {
      "outputs": ["node_modules/.cache/tsbuildinfo.json"]
    },
    "deps:check": {
      "cache": false
    },
    "test": {
      "cache": false,
      "outputs": ["coverage/**"],
      "inputs": ["$TURBO_DEFAULT$", ".env"]
    }
  }
}
